      <select id="command">
        <option value="don">ドンッ！</option>
      </select>
      <button type="button" id="soundCommandButton">音声コマンド送信</button>
      <div id="rec-log">音声認識：<span id="rec-state"></span></div>
      <input type="checkbox" id="saveCanvas"><label for="saveCanvas">スナップショットを撮る</label>
      <br><a href="http://zapper.kakkoii.tv/?eventId=51ecca6063b71c4b39000012" target="_blank">かっこいいねボタン</a>
    </form>


